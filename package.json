{
  "name": "cdk-db-migration",
  "description": "CDK Construct for managing DB migrations",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "author": {
    "name": "Daniel Schroeder",
    "url": "https://www.udondan.com/",
    "twitter": "udondan"
  },
  "awscdkio": {
    "twitter": "udondan"
  },
  "homepage": "https://github.com/udondan/cdk-db-migration",
  "repository": {
    "type": "git",
    "url": "https://github.com/udondan/cdk-db-migration.git"
  },
  "bugs": {
    "url": "https://github.com/udondan/cdk-db-migration/issues"
  },
  "keywords": [
    "aws",
    "cdk",
    "aws-cdk",
    "athena",
    "db",
    "migration"
  ],
  "stability": "stable",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii -w",
    "package": "jsii-pacmak",
    "prepackage": "jsii && lambda/build"
  },
  "jsii": {
    "excludeTypescript": [
      "lambda",
      "test/*"
    ],
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "cdk-db-migration",
        "module": "cdk_db_migration"
      }
    }
  },
  "devDependencies": {
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0",
    "@types/aws-lambda": "^8.10.97",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.7.9",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1134.0",
    "jsii": "^1.58.0",
    "jsii-pacmak": "^1.58.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "aws-cloudformation-custom-resource": "^3.1.0"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0"
  }
}
