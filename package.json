{
  "name": "cdk-db-migration",
  "description": "CDK Construct for managing DB migrations",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "author": {
    "name": "Daniel Schroeder",
    "url": "https://www.udondan.com/",
    "twitter": "udondan"
  },
  "awscdkio": {
    "twitter": "udondan"
  },
  "homepage": "https://github.com/udondan/cdk-db-migration",
  "repository": {
    "type": "git",
    "url": "https://github.com/udondan/cdk-db-migration.git"
  },
  "bugs": {
    "url": "https://github.com/udondan/cdk-db-migration/issues"
  },
  "keywords": [
    "aws",
    "cdk",
    "aws-cdk",
    "athena",
    "db",
    "migration"
  ],
  "stability": "stable",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii -w",
    "package": "jsii-pacmak",
    "prepackage": "jsii && lambda/build"
  },
  "jsii": {
    "excludeTypescript": [
      "lambda"
    ],
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "cdk-db-migration",
        "module": "cdk_db_migration"
      },
      "dotnet": {
        "namespace": "CDK.DB.Migration",
        "packageId": "CDK.DB.Migration"
      }
    }
  },
  "devDependencies": {
    "@aws-cdk/aws-cloudformation": "^1.50.0",
    "@aws-cdk/aws-glue": "^1.50.0",
    "@aws-cdk/aws-iam": "^1.50.0",
    "@aws-cdk/aws-lambda": "^1.50.0",
    "@aws-cdk/core": "^1.50.0",
    "@types/aws-lambda": "^8.10.72",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^14.14.35",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.866.0",
    "jsii": "^1.25.0",
    "jsii-pacmak": "^1.25.0",
    "ts-node": "^9.1.1",
    "typescript": "~4.2.3"
  },
  "dependencies": {},
  "peerDependencies": {
    "@aws-cdk/aws-cloudformation": "^1.50.0",
    "@aws-cdk/aws-glue": "^1.50.0",
    "@aws-cdk/aws-iam": "^1.50.0",
    "@aws-cdk/aws-lambda": "^1.50.0",
    "@aws-cdk/core": "^1.50.0",
    "constructs": "^3.2.80"
  }
}
